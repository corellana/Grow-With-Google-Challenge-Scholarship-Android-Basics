<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carito.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Question1"
            android:label="Question One"
            />
        <activity
            android:name=".Question2"
            android:label="Question Two"/>

        <activity
            android:name=".ScoreActivity"
            android:label="Your final Score"
            />
        <activity
            android:name=".Question3"
            android:label="Question Three"/>
        <activity
            android:name=".Question4"
            android:label="Question Four"/>
        <activity
            android:name=".Question5"
            android:label="Question Five">

        </activity>

    </application>

</manifest>